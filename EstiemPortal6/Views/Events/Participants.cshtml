@model IQueryable<EstiemPortal6.ViewModels.ParticipantsViewModel>


<h2>Participants of @Model.First().EventName</h2>

<div class="row">
    <div class="col-sm-3">
        Name
    </div>
    <div class="col-sm-3">
        Local Group
    </div>
    <div class="col-sm-3">
       Registration Status
    </div>
    <div class="col-sm-3">
        Application Date
    </div>
</div>
@foreach (var pax in Model)
{
    <div class="row">
        <div class="col-sm-3">
            @pax.Name
        </div>
        <div class="col-sm-3">
            @pax.LocalGroup
        </div>
        <div class="col-sm-3">
            @pax.RegistrationStatus
        </div>
        <div class="col-sm-3">
            @pax.ApplicationDate
        </div>
    </div>
}
